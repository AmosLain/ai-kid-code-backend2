{
  "name": "story-generator-pro",
  "version": "2.0.0",
  "description": "AI-powered story generator with search and video creation capabilities",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "setup": "npm install && mkdir -p logs uploads public/videos",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "build": "echo 'No build step required for this project'",
    "docker:build": "docker build -t story-generator-pro .",
    "docker:run": "docker run -p 3000:3000 story-generator-pro"
  },
  "keywords": [
    "story-generator",
    "ai",
    "kids",
    "education",
    "search",
    "video-generation",
    "express",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "sharp": "^0.32.6",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "canvas": "^2.11.2",
    "fuse.js": "^7.0.0",
    "natural": "^6.8.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "sanitize-html": "^2.11.0",
    "rate-limiter-flexible": "^4.0.1",
    "express-slow-down": "^2.0.1",
    "express-fileupload": "^1.4.3",
    "nodemailer": "^6.9.7",
    "csv-parser": "^3.0.0",
    "xml2js": "^0.6.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "@types/node": "^20.9.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/story-generator-pro.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/story-generator-pro/issues"
  },
  "homepage": "https://github.com/yourusername/story-generator-pro#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error"
    }
  }
}
